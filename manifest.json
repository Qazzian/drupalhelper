{
  "name": "Drupal Helper",
  "version": "0.5",
  "description": "Provides convenience stuff for drupal.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "*://*/admin/modules"
  ],
  "content_scripts" : [
      {
        "matches" : [
          "*://*/admin/modules",
          "*://*/admin/modules/list"
        ],
        "css" : ["modules.css"],
        "js" : ["modules.js"]
      },
      {
        "matches" : [
          "*://*/admin/people/permissions"
        ],
        "css" : ["permissions.css"],
        "js" : ["permissions.js"]
      }
  ],
  "icons" : {
    "128" : "icon-128.png"
  },
  "manifest_version" : 2
}